<div class="right-align">
  <%= link_to new_group_presentation_path(current_group), class: 'btn-floating' do %>
    <i class="material-icons">add</i>
  <% end %>
</div>

<%= render partial: "presentations/search_form" %>

<div class="row">
  <% @presentations.each do |presentation| %>
      <div class="col s12 m6">
        <%= render partial: "presentations/presentation_card", locals: { presentation: presentation } %>
      </div>
  <% end %>
</div>
